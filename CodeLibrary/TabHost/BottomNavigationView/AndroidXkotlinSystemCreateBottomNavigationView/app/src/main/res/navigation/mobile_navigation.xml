<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.jackshi.androidx_kotlin.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_find"
        android:name="com.jackshi.androidx_kotlin.ui.find.FindFragment"
        android:label="@string/title_find"
        tools:layout="@layout/fragment_find" />

    <fragment
        android:id="@+id/navigation_message"
        android:name="com.jackshi.androidx_kotlin.ui.message.MessageFragment"
        android:label="@string/title_message"
        tools:layout="@layout/fragment_message" />

    <fragment
        android:id="@+id/navigation_member"
        android:name="com.jackshi.androidx_kotlin.ui.member.MemberFragment"
        android:label="@string/title_member"
        tools:layout="@layout/member_fragment" />

    <fragment
        android:id="@+id/navigation_me"
        android:name="com.jackshi.androidx_kotlin.ui.me.MeFragment"
        android:label="@string/title_me"
        tools:layout="@layout/me_fragment" />
</navigation>